    </div>
    <script>
        function initMap() {
            var utown = {lat: 1.3046, lng: 103.7723}; //u-town: 1.3046° N, 103.7723° E
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 18,
                center: utown
            });
            var marker = new google.maps.Marker({
                position: utown,
                map: map
            });
        }
    </script>
