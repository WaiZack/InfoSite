<section id="statement" class="layer">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">

        <h1 class="section-heading">Workshops</h1>



            <div class="row">
              <div class="col-md-2"></div>

              <div class="col-md-8">
                <ul class="event-list">


                  <li>
                    <time datetime="2017-06-02">
                      <span class="day">2</span>
                      <span class="month">Jun</span>
                      <span class="year">2017</span>
                      <span class="time">ALL DAY</span>
                    </time>
                    <%= image_tag 'workshops/cfg.jpg' %>
                    <div class="info">
                      <h2 class="title">Personal Branding & Networking Workshop</h2>
                      <div class="row">
                        <span class="fa fa-clock-o"></span>    2nd June, 10AM - 12 PM
                      </div>
                      <div class="row">
                        <span class="fa fa-money"></span>    Free
                      </div>
                      <div class="row">
                        <span class="fa fa-map-marker"></span>    <a href="http://www.comp.nus.edu.sg/maps/photos/#com1" target="_blank">
                        Cerebro Room, COM1, 13 Computing Drive, Singapore 117417</a>
                      </div>
                      <div class="row">
                        <span class="fa fa-hand-o-right"></span>    <a href="#PBNW" class="portfolio-link" data-toggle="modal">Find out more</a>
                        &nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp<span class="fa fa-hand-o-right"></span>    <a href="https://www.eventbrite.sg/e/personal-branding-networking-workshop-tickets-34513098607" target="_blank">Register</a>
                      </div>
                    </div>

                  </li>



                  <li>
                    <time datetime="2017-06-08">
                      <span class="day">8-9</span>
                      <span class="month">Jun</span>
                      <span class="year">2017</span>
                      <span class="time">ALL DAY</span>
                    </time>
                    <%= image_tag 'home/sas.png' %>
                    <div class="info">
                      <h2 class="title">SAS Viya Workshop</h2>
                      <div class="row">
                        <span class="fa fa-clock-o"></span>    8th - 9th June, 9AM - 6PM
                      </div>
                      <div class="row">
                        <span class="fa fa-money"></span>    Free
                      </div>
                      <div class="row">
                        <span class="fa fa-map-marker"></span>    To Be Confirmed
                      </div>
                    </div>
                  </li>



                </ul>
              </div>

              <div class="col-md-2"></div>
            </div>


      </div>
    </div>
  </div>
</section>


<!-- Modal 1 -->
<div class="portfolio-modal modal fade" id="PBNW" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="close-modal" data-dismiss="modal">
        <div class="lr">
          <div class="rl">
          </div>
        </div>
      </div>
      <div class="container">
        <div class="row">
          <div class="col-md-8 col-md-offset-2">
            <div class="modal-body">

              <div class="row">
                <div class="portfolioDiv">
                  <%= image_tag 'workshops/cfg.jpg' %>
                  <h4><span>Personal Branding & Networking Workshop</span></h4>
                  <h5>Impart critical skills for your future career</h5>
                </div>
              </div>

              <div class = "row portfolioDiv">
                <p class="text-justify">
                  Specially designed to ignite your thinking about future career options. Be poised and ready for your
                  first foray into the working world as we walk you through the process and impart practical skills to
                  you through this experiential workshops to equip you with Personal Branding and Networking techniques.
                  <br><br>*This Workshop is organised exclusively for participants of BiZiT Festival 2017.
                </p>
              </div>
              <div class = "nextRow">
                <button type="button" class="btn btn-primary" data-dismiss="modal"><i class="fa fa-times"></i> Close Portfolio</button>
              </div>

            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>



