<!--talk timeline-->
<section class="layer">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">

          <ul class="timeline">
            <li><div class="tldate">8.30 ~ 9.00am Registration</div></li>

            <li>
              <div class="tl-circ"></div>
              <div class="timeline-panel">
                <div class="tl-heading">
                  <h4>Opening talk on Fintech</h4>
                  <h3><span class="fa fa-clock-o"></span> 9.00 ~ 9.45am</h3>
                </div>
                <div class="tl-body">
                  <p>Speaker: MAS </p>
                </div>
              </div>
            </li>

            <li class="timeline-inverted">
              <div class="tl-circ"></div>
              <div class="timeline-panel">
                <div class="tl-heading">
                  <h4>Panel 1: Fintech Innovation and the Singapore Business Environment</h4>
                  <h3><span class="fa fa-clock-o"></span> 9.45 ~ 10.30am</h3>
                </div>
                <div class="tl-body">
                  <p>Panelists:
                      <br>Prof. Huang Ke‐Wei
                      <br>Mr. Anupam Pahuja
                      <br>Mr. Alan Lim
                      <br>Ms. Judy Hsu
                  </p>
                </div>
              </div>
            </li>

            <li><div class="tldate">10.30 ~ 11.00am Tea Break</div></li>

            <li>
              <div class="tl-circ"></div>
              <div class="timeline-panel">
                <div class="tl-heading">
                  <h4>Talk on M‐commerce</h4>
                  <h3><span class="fa fa-clock-o"></span> 11.00 ~ 11.45am</h3>
                </div>
                <div class="tl-body">
                  <p>Speaker: Prof. Anindya Ghose</p>
                </div>
              </div>
            </li>

            <li class="timeline-inverted">
              <div class="tl-circ"></div>
              <div class="timeline-panel">
                <div class="tl-heading">
                  <h4>Panel 2: Advancing Digital Commerce through Customer Analytics</h4>
                  <h3><span class="fa fa-clock-o"></span> 11.45 ~ 12.30pm</h3>
                </div>
                <div class="tl-body">
                  <p>Panelists:
                    <br>Prof. Teo Hock Hai
                    <br>Prof. Arun Rai
                    <br>Prof. Bin Gu
                    <br>Ms. Gan Siok Hoon
                    <br>Ms. Silvia Thom
                  </p>
                </div>
              </div>
            </li>

            <li><div class="tldate">12.30 ~ 2.00pm Lunch</div></li>

            <li>
              <div class="tl-circ"></div>
              <div class="timeline-panel">
                <div class="tl-heading">
                  <h4>Talk on Smart Nation</h4>
                  <h3><span class="fa fa-clock-o"></span> 2.00 ~ 2.45pm</h3>
                </div>
                <div class="tl-body">
                  <p>Speaker: Accenture</p>
                </div>
              </div>
            </li>


            <li class="timeline-inverted">
              <div class="tl-circ"></div>
              <div class="timeline-panel">
                <div class="tl-heading">
                  <h4>Panel 3: Smart Nation: Instrumented, Innovative, and Participatory</h4>
                  <h3><span class="fa fa-clock-o"></span> 2.45 ~ 3.30pm</h3>
                </div>
                <div class="tl-body">
                  <p>Panelists:
                    <br>Prof. Atreyi Kankanhalli
                    <br>Prof. Alex Siow
                    <br>Prof. Elena Karahanna
                    <br>Mr. Daniel Teo
                  </p>
                </div>
              </div>
            </li>

            <li><div class="tldate">3.30 ~ 4.00pm Tea Break</div></li>

            <li>
              <div class="tl-circ"></div>
              <div class="timeline-panel">
                <div class="tl-heading">
                  <h4>BiZiT Challenge Finalist Presentations</h4>
                  <h3><span class="fa fa-clock-o"></span> 4.00 ~ 5.00pm</h3>
                </div>
              </div>
            </li>

            <li>
              <div class="timeline-panel noarrow">
                <div class="tl-heading">
                  <h4>Guest of Honour Speech</h4>
                  <h3><span class="fa fa-clock-o"></span> 5.00 ~ 5.30pm</h3>
                </div>
              </div>
            </li>

            <li class="timeline-inverted">
              <div class="tl-circ"></div>
              <div class="timeline-panel">
                <div class="tl-heading">
                  <h4>BiZiT Challenge Awards Ceremony/Photo Taking</h4>
                  <h3><span class="fa fa-clock-o"></span> 5.30 ~ 6.00pm</h3>
                </div>
              </div>
            </li>

            <li class="timeline-inverted">
              <div class="timeline-panel noarrow">
                <div class="tl-heading">
                  <h4>Dinner and Networking</h4>
                  <h3><span class="fa fa-clock-o"></span> 6.00 ~ 7.00pm</h3>
                </div>
              </div>
            </li>
          </ul>

      </div>
    </div>
  </div>
</section>

